{
  "expo": {
    "name": "UnfilteredByte",
    "slug": "Emotions",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logowithoutname.png",
    "backgroundColor": "#000000",
    "scheme": "com.unfilteredbyte",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera to take photos for posts",
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos to upload them to posts",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos to your gallery",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.UnfilteredByte.Byte"
    },
    "android": {
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logowithoutname.png",
        "backgroundColor": "#000000"
      },
      "package": "com.UnfilteredByte.Byte"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logowithoutname.png",
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logowithoutname.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you take photos to share with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d7881303-1379-4a3a-8d67-88b921b673c8"
      }
    },
    "owner": "bhanupratap21"
  }
}